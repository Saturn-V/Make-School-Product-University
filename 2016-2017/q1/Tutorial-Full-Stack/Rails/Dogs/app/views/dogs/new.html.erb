<%= form_for(@dog) do |f| %>
<div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
</div>

<div class="field">
    <%= f.label :breed %>
    <%= f.text_field :breed %>
</div>

<div class="field">
    <%= f.label :price_hr %>
    <%= f.text_field :price_hr %>
</div>

<div class="actions">
    <%= f.submit %>
</div>
<% end %>

